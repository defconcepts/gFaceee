{
    "name": "gFaceee",
    "version": "0.5.1",
    "description": "Display avatar following on GitHub News Feed.",
    "permissions": ["storage", "*://*.github.com/*"],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*.github.com/"],
        "css": ["extensions.css"],
        "js": ["lib/jquery.js", "lib/underscore.js", "extensions.js"]
    }],
    "web_accessible_resources": [
        "img/default_avatar.png"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
}