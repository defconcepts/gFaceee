{
  "name": "gFaceee",
  "version": "0.8.0",
  "description": "Chrome Extension for displaying avatar on GitHub feed.",
  "permissions": [
    "storage",
    "*://*.github.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/"
      ],
      "css": [
        "contents.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "lib/underscore-min.js",
        "lib/image-encoder.js",
        "contents.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}