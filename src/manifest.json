{
  "name": "gFaceee",
  "version": "0.9.1",
  "description": "A Chrome Extension for displaying avatar on GitHub feed.",
  "permissions": [
    "storage",
    "*://*.github.com/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "css": [
        "css/contents.css"
      ],
      "js": [
        "js/lib/image-encoder.js",
        "js/lib/fetch.js",
        "js/dist/contents.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}